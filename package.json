	"license": "GPL-3.0",
	"bugs": {
		"url": "https://gitlab.com/Gigadrive/twitcharchive/twitcharchive/issues"
	},
	"homepage": "https://gitlab.com/Gigadrive/twitcharchive/twitcharchive#readme",
	"dependencies": {
		"bootstrap": "^4.4.1",
		"file-loader": "^5.0.2",
		"jquery": "^3.4.1",
		"less": "^3.10.3",
		"less-loader": "^5.0.0",
		"mini-css-extract-plugin": "^0.8.0",
		"node-sass": "^4.13.0",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
