		"build": "npm run clean && webpack --config=webpack.prod.js",
		"watch": "npm run clean && webpack --config=webpack.dev.js",
		"clean": "node clean.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://gitlab.com/Gigadrive/twitcharchive/twitcharchive.git"
	},
	"keywords": [
